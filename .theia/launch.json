  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "env": {
        "DJANGO_SETTINGS_MODULE": "locallibrary.local_settings"
